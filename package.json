{
  "name": "react-styleable",
  "description": "Higher-order component for styling react",
  "main": "index.js",
  "version": "0.0.0-semantically-released",
  "scripts": {
    "commit": "git-cz",
    "compile": "babel src/ --source-maps --debug --out-dir lib/",
    "prepublish": "npm run compile",
    "test": "mocha \"src/**/*.spec.js\"",
    "test-watch": "mocha \"src/**/*.spec.js\" --watch",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "keywords": [
    "react",
    "style",
    "react-style",
    "local",
    "css",
    "icss",
    "module",
    "inline styles",
    "css modules",
    "css-module"
  ],
  "author": "jaketrent",
  "repository": {
    "type": "git",
    "url": "https://github.com/pluralsight/react-styleable.git"
  },
  "license": "MIT",
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0"
  },
  "dependencies": {
    "invariant": "^2.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "commitizen": "^2.4.6",
    "cz-conventional-changelog": "^1.1.5",
    "jsdom": "^7.2.2",
    "mocha": "^2.3.3",
    "react": "^15.5.4",
    "prop-types": "15.5.8",
    "react-dom": "^15.5.4",
    "rf-release": "^0.4.0",
    "semantic-release": "^4.3.5",
    "should": "^7.0.1"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
